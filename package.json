{
  "name": "electron-release-binary",
  "productName": "Release Binary",
  "version": "0.0.1",
  "description": "Release Binary app",
  "main": "src/main.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\""
  },
  "repository": "",
  "keywords": [],
  "author": {
    "name": "PinShengJuan",
    "email": "pinshengjuan@gmail.com"
  },
  "license": "CC0-1.0",
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "ico/icons/win/icon.ico"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-wix",
          "config": {
            "name": "Release Binary",
            "exe": "Release Binary",
            "features": {
              "autoUpdate": true,
              "autoLaunch": true
            },
            "arch": "x64"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "devDependencies": {
    "@electron-forge/maker-deb": "^6.0.0-beta.69",
    "@electron-forge/maker-rpm": "^6.0.0",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.69",
    "@electron-forge/maker-wix": "6.0.0-beta.74",
    "@electron-forge/maker-zip": "^6.0.0-beta.69",
    "electron": "^11.3.0"
  },
  "dependencies": {
    "@electron-forge/cli": "^6.0.0-beta.69",
    "electron-forge-maker-nsis": "^24.0.0-alpha.3",
    "electron-icon-maker": "^0.0.5",
    "electron-squirrel-startup": "^1.0.0",
    "electron-wix-msi": "^5.0.0",
    "python-shell": "^2.0.3"
  }
}
